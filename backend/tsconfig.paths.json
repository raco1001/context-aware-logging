    "baseUrl": "./",
    "paths": {
      "@logging/*": ["libs/logging/*"],
      "@logging/domain/*": ["libs/logging/core/domain/*"],
      "@logging/value-objects/*": ["libs/logging/core/value-objects/*"],
      "@logging/out-ports/*": ["libs/logging/core/ports/out/*"],
      "@logging/in-ports/*": ["libs/logging/core/ports/in/*"],
